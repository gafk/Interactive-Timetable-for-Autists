<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    xmlns:local="http://schemas.android.com/apk/res-auto"
    p1:layout_width="fill_parent"
    p1:layout_height="fill_parent">
    <include
        p1:id="@+id/m_toolbar"
        layout="@layout/toolbar"
        p1:layout_alignParentTop="true" />
    <LinearLayout
        p1:orientation="horizontal"
        p1:minWidth="25px"
        p1:minHeight="25px"
        p1:layout_width="match_parent"
        p1:layout_height="match_parent"
        p1:id="@+id/main_landscape"
        p1:layout_below="@id/m_toolbar">
        <LinearLayout
            p1:orientation="vertical"
            p1:minWidth="25px"
            p1:minHeight="25px"
            p1:layout_width="400dp"
            p1:layout_height="match_parent"
            p1:id="@+id/user_list" />
        <LinearLayout
            p1:orientation="vertical"
            p1:minWidth="25px"
            p1:minHeight="25px"
            p1:layout_width="match_parent"
            p1:layout_height="match_parent"
            p1:id="@+id/user_details_general">
            <TextView
                p1:text="Карточка ребенка"
                p1:textAppearance="?android:attr/textAppearanceLarge"
                p1:layout_width="match_parent"
                p1:layout_height="wrap_content"
                p1:id="@+id/user_card_label"
                p1:layout_marginTop="30dp"
                p1:textSize="25dp"
                p1:gravity="center" />
            <View
                p1:layout_width="fill_parent"
                p1:layout_height="2dip"
                p1:background="@color/vivid_blue"
                p1:layout_marginLeft="300dp"
                p1:layout_marginRight="350dp"
                p1:layout_marginTop="10dp"
                p1:id="@+id/user_card_divider" />
            <LinearLayout
                p1:orientation="vertical"
                p1:minWidth="25px"
                p1:minHeight="25px"
                p1:layout_width="match_parent"
                p1:layout_height="wrap_content"
                p1:id="@+id/user_details_and_trips"
                p1:layout_marginTop="30dp">
                <FrameLayout
                    p1:minWidth="25px"
                    p1:minHeight="25px"
                    p1:layout_width="match_parent"
                    p1:layout_height="wrap_content"
                    p1:id="@+id/user_details"
                    p1:layout_marginLeft="400dp"
                    p1:layout_marginRight="400dp" />
                <TextView
                    p1:text="@string/trips"
                    p1:textAppearance="?android:attr/textAppearanceLarge"
                    p1:layout_width="match_parent"
                    p1:layout_height="wrap_content"
                    p1:id="@+id/trips_label"
                    p1:gravity="center"
                    p1:layout_marginTop="200dp"
                    p1:textSize="25dp" />
                <View
                    p1:layout_width="fill_parent"
                    p1:layout_height="2dip"
                    p1:background="@color/vivid_blue"
                    p1:layout_marginLeft="300dp"
                    p1:layout_marginRight="350dp"
                    p1:layout_marginTop="10dp"
                    p1:id="@+id/trips_divider" />
                <FrameLayout
                    p1:minWidth="25px"
                    p1:minHeight="25px"
                    p1:layout_width="match_parent"
                    p1:layout_height="wrap_content"
                    p1:id="@+id/user_trips">
                    <LinearLayout
                        p1:orientation="horizontal"
                        p1:minWidth="25px"
                        p1:minHeight="25px"
                        p1:layout_width="match_parent"
                        p1:layout_height="match_parent"
                        p1:id="@+id/linearLayout1">
                        <FrameLayout
                            p1:minWidth="25px"
                            p1:minHeight="25px"
                            p1:layout_width="300dp"
                            p1:layout_height="500dp"
                            p1:id="@+id/trip_list_frame"
                            p1:layout_marginLeft="400dp"
                            p1:layout_marginTop="30dp">
                            <LinearLayout
                                p1:orientation="vertical"
                                p1:minWidth="25px"
                                p1:minHeight="25px"
                                p1:layout_width="match_parent"
                                p1:layout_height="match_parent"
                                p1:id="@+id/trip_list" />
                        </FrameLayout>
                        <LinearLayout
                            p1:orientation="vertical"
                            p1:minWidth="25px"
                            p1:minHeight="25px"
                            p1:layout_width="997dp"
                            p1:layout_height="wrap_content"
                            p1:id="@+id/trip_detailed_info"
                            p1:layout_marginLeft="50dp"
                            p1:layout_gravity="center" />
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>